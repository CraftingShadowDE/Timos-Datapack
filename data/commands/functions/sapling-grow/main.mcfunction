execute as @e[type=minecraft:area_effect_cloud,tag=SaplingGrowTester2,nbt={Age:1}] at @s unless block ~ ~ ~ #commands:saplings-2x2-possible run summon minecraft:area_effect_cloud ~ ~ ~ {NoGravity:1b,Radius:0f,Duration:60,Tags:["SaplingGrow","SaplingGrowActor","SaplingGrowActor1x1"],Invulnerable:1b}
#remove Actor1x1 because 2x2 necessary but not found:
execute as @e[type=minecraft:area_effect_cloud,tag=SaplingGrowActor1x1,nbt={Age:1}] at @s if block ~ ~ ~ #commands:saplings-2x2-necessary run kill @s

#kill Tester's (at Age 3)
execute as @e[type=minecraft:area_effect_cloud,tag=SaplingGrowTester,nbt={Age:3}] at @s run kill @s

#if multiple Actor's at one place, kill all but one random chosen one; if there are 1x1 actors too, they will alwasy lose (at Age 3)
#execute as @e[type=minecraft:area_effect_cloud,tag=SaplingGrowActor,nbt={Age:3}] at @s if entity @e[type=minecraft:area_effect_cloud,tag=SaplingGrowActor,distance=..1.5] as @e[type=minecraft:area_effect_cloud,tag=SaplingGrowActor,distance=..1.5,sort=arbitrary,limit=1] at @s run kill @e[type=minecraft:area_effect_cloud,tag=SaplingGrowActor,distance=0.01..1.5]
execute as @e[type=minecraft:area_effect_cloud,tag=SaplingGrowActor2x2,nbt={Age:3}] at @s as @e[type=minecraft:area_effect_cloud,tag=SaplingGrowActor2x2,limit=1,dx=1,dz=1,dy=0,nbt={Age:3},sort=arbitrary] run tag @s add SaplingGrowActorSurvivor
execute as @e[type=minecraft:area_effect_cloud,tag=SaplingGrowActorSurvivor,nbt={Age:3}] at @s run kill @e[type=minecraft:area_effect_cloud,tag=SaplingGrowActor,nbt={Age:3},dx=1,dz=1,dy=0,tag=!SaplingGrowActorSurvivor]
execute as @e[type=minecraft:area_effect_cloud,tag=SaplingGrowActorSurvivor,nbt={Age:3}] at @s run tag @s remove SaplingGrowActorSurvivor

#effects at Actor's (at Age 4)
execute as @e[type=minecraft:area_effect_cloud,tag=SaplingGrowActor,nbt={Age:4}] at @s run particle minecraft:happy_villager ~ ~ ~ 0.25 0.25 0.25 1 10 force
execute as @e[type=minecraft:area_effect_cloud,tag=SaplingGrowActor,nbt={Age:4}] at @s run playsound minecraft:item.bone_meal.use master @a ~ ~ ~ 1

#let trees grow with a specific chance (predicate commands:tree_grow_chance) (at Age 5)
execute as @e[type=minecraft:area_effect_cloud,tag=SaplingGrowActor,nbt={Age:5}] at @s if predicate commands:tree_grow_chance run function commands:sapling-grow/grow
#fix dark_oak (at Age 6)
execute as @e[type=minecraft:area_effect_cloud,tag=SaplingGrowActor,nbt={Age:6}] at @s if score @s timo_actorType matches 2 if score @s timo_growSuccess matches 1 if score @s timo_sapling-type matches 6 run fill ~ ~ ~ ~1 ~ ~1 dark_oak_log[axis=y]

#kill Actors at age 30 (=> 1.5 second cooldown)
execute as @e[type=minecraft:area_effect_cloud,tag=SaplingGrowActor,nbt={Age:30}] at @s run kill @s